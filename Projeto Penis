int mostrarMap() {

    int domino[7][7] = {0}; 


    for (int i = 0; i <= 6; i++) {
    	
        for (int j = i; j <= 6; j++) {
        	
            domino[i][j] = i;
            
            domino[j][i] = j;
            
            printf("[%d|%d] ", i, j);
        }
        printf("\n");
    }

  
}
